{
  "name": "pantry-cloud",
  "description": "NodeJS package for Pantry",
  "version": "0.0.10",
  "license": "MIT",
  "author": "RÃ³bert Darida",
  "keywords": [
    "pantry",
    "npm",
    "package"
  ],
  "funding": [
    {
      "type": "patreon",
      "url": "https://www.patreon.com/rdarida"
    }
  ],
  "homepage": "https://rdarida.github.io/pantry-cloud",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdarida/pantry-cloud.git"
  },
  "bugs": {
    "url": "https://github.com/rdarida/pantry-cloud/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "pantry": "dist/cli.js"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "clean": "rimraf coverage dist docs",
    "format": "prettier --write \"**/{src,test}/*.{ts,json}\"",
    "lint": "eslint ./{src,test}/**/*.ts",
    "pretest": "rimraf coverage",
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/cli.js",
    "predocs": "rimraf docs",
    "docs": "typedoc",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin main",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^15.3.1",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "jest": "^26.6.3",
    "jest-config": "^27.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.2.0",
    "ts-jest": "^26.5.5",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4"
  }
}
