{
  "name": "pantry-cloud",
  "description": "NodeJS package for Pantry",
  "version": "0.1.8",
  "license": "MIT",
  "author": "RÃ³bert Darida",
  "keywords": [
    "pantry",
    "npm",
    "package"
  ],
  "funding": [
    {
      "type": "patreon",
      "url": "https://www.patreon.com/rdarida"
    }
  ],
  "homepage": "https://rdarida.github.io/pantry-cloud",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdarida/pantry-cloud.git"
  },
  "bugs": {
    "url": "https://github.com/rdarida/pantry-cloud/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "pantry": "dist/cli.js"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "clean": "rimraf coverage dist docs",
    "format": "lint-staged",
    "xformat": "prettier --write \"**/{src,test}/*.{ts,json}\"",
    "lint": "eslint ./{src,test}/**/*.ts",
    "pretest": "rimraf coverage",
    "test": "jest",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/cli.js",
    "predocs": "rimraf docs",
    "docs": "typedoc",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin HEAD",
    "prepublishOnly": "npm run build",
    "echo": "echo test"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "dotenv": "^14.2.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "@types/yargs": "^16.0.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.4.5",
    "jest-config": "^28.0.2",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "**/{src,test}/*.{ts,json}": [
      "prettier --write"
    ]
  }
}
